<!ELEMENT milk (static-path?, encoding?, initial-capacity?, load-factor?,app*)>

    <!ELEMENT static-path (#PCDATA)>

    <!ELEMENT encoding (#PCDATA)>

    <!ELEMENT initial-capacity (#PCDATA)>

    <!ELEMENT load-factor (#PCDATA)>

    <!ELEMENT app (app-name,urls)>

        <!ELEMENT app-name (#PCDATA)>

        <!ELEMENT urls (url+)>

            <!ELEMENT url (url-name,url-service?,url-template?)>

                <!ELEMENT url-name (#PCDATA)>

                <!ELEMENT url-service (#PCDATA)>

                <!ELEMENT url-template (#PCDATA)>

