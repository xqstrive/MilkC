<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE milk SYSTEM "https://raw.githubusercontent.com/xqstrive/MilkC/master/milk_1_0.dtd">

<milk>
    <static-path></static-path>
    <encoding>UTF-8</encoding>
    <initial-capacity>16</initial-capacity>
    <load-factor>0.75f</load-factor>



    <app>
        <app-name>/</app-name>
        <urls>
            <url>
                <url-name>/</url-name>
                <url-service>app.ServiceHelloWorld</url-service>
                <url-template>\template\test1.html</url-template>
            </url>
            <url>
                <url-name>/index</url-name>
                <url-service></url-service>
                <url-template>\template\index.html</url-template>
            </url>
            <url>
                <url-name>/sign</url-name>
                <url-service></url-service>
                <url-template>\template\sign.html</url-template>
            </url>
        </urls>
    </app>

</milk>
